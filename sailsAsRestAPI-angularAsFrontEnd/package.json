{
  "name": "SailsAngular",
  "appName": "SailsAngular",
  "version": "0.0.0",
  "description": "Sails js as rest API and angular as Front End Application",
  "license": "MIT",
  "main": "app.js",
  "repository": "jasancheg/electronApps",
  "author": {
    "name": "Jose Antonio Sanchez",
    "email": "your@email.com",
    "url": "inideaweb.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --all --version=0.30.0 --icon=../icons/smile_128x128.ico --overwrite",
    "build-mac": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --version=0.30.0 --icon=../icons/smile_128x128.ico --platform=darwin --arch=x64 --overwrite",
    "build-win32": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --version=0.30.0 --icon=../icons/smile_128x128.ico --platform=win32 --arch=ia32 ",
    "build-win64": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --version=0.30.0 --icon=../icons/smile_128x128.ico --platform=win32 --arch=x64 --overwrite",
    "build-linux32": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --version=0.30.0 --icon=../icons/smile_128x128.ico --platform=linux --arch=ia32 --overwrite",
    "build-linux64": "electron-packager ./ SailsAngular --out=builds --ignore=builds --prune --asar --version=0.30.0 --icon=../icons/smile_128x128.ico --platform=linux --arch=x64 --overwrite"
  },
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "electron-debug": "^0.1.1",
    "titlebar": "^1.3.0"
  },
  "devDependencies": {
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.30.0"
  }
}
